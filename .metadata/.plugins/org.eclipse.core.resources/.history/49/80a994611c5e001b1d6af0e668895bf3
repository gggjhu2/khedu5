package practice;

public class ROUND {
public static void main(String[] args) {

	for(int i = 1; i <= 9; i++) {
		for(int j=1; j <= 9; j++) {
			System.out.println(i + " * " + j + " = " + i * j);
			} System.out.println(); 
			} 

	
	System.out.println("내가만든 구구단 ");
	
	for(int dan =1 ; dan<=9;dan++) {
		for(int j =1;j<=9;j++) {
			
			System.out.println(dan+"단:"+dan+"X"+j+" ="+dan*j);
		}
	}
}
}

